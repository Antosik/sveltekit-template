{
	"name": "sveltekit-template",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite --port 3001",
		"build": "vite build",
		"build:dev": "vite build --mode development",
		"postinstall": "npm run kit:sync",
		"preview": "vite preview --port 3001",
		"lint": "concurrently -g \"npm:lint:svelte\" \"npm:lint:ts\" \"npm:lint:eslint\" \"npm:lint:css\"",
		"lint:fix": "concurrently -g \"npm:lint:svelte\" \"npm:lint:ts\" \"npm:lint:eslint:fix\" \"npm:lint:css:fix\"",
		"lint:svelte": "svelte-check --tsconfig ./tsconfig.json",
		"lint:css": "stylelint \"src/**/*.{svelte,css,scss}\"",
		"lint:css:fix": "stylelint \"src/**/*.{svelte,css,scss}\" --fix",
		"lint:ts": "tsc --noemit",
		"lint:eslint": "eslint \"src/**/*.{svelte,js,mjs,cjs,ts}\"",
		"lint:eslint:fix": "eslint \"src/**/*.{svelte,js,mjs,cjs,ts}\" --fix",
		"format": "prettier --write --plugin prettier-plugin-svelte \"src/**/*.{svelte,js,cjs,mjs,ts,css,scss,html,json}\"",
		"kit:sync": "svelte-kit sync"
	},
	"dependencies": {
		"feather-icons": "4.29.1"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "3.0.1",
		"@sveltejs/kit": "2.5.7",
		"@total-typescript/ts-reset": "0.5.1",
		"@types/feather-icons": "4.29.4",
		"@types/node": "20.12.7",
		"@typescript-eslint/eslint-plugin": "7.7.0",
		"@typescript-eslint/parser": "7.7.0",
		"autoprefixer": "10.4.19",
		"concurrently": "8.2.2",
		"eslint": "8.56.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-import-resolver-typescript": "3.6.1",
		"eslint-plugin-import": "2.29.1",
		"eslint-plugin-prettier": "5.1.3",
		"eslint-plugin-svelte": "2.37.0",
		"postcss": "8.4.38",
		"postcss-load-config": "5.0.3",
		"prettier": "3.2.5",
		"prettier-plugin-svelte": "3.2.3",
		"sass": "1.75.0",
		"stylelint": "16.3.1",
		"stylelint-config-html": "1.1.0",
		"stylelint-config-idiomatic-order": "10.0.0",
		"stylelint-config-prettier-scss": "1.0.0",
		"stylelint-config-standard-scss": "13.1.0",
		"stylelint-order": "6.0.4",
		"stylelint-scss": "6.2.1",
		"svelte": "4.2.15",
		"svelte-check": "3.6.9",
		"svelte-preprocess": "5.1.4",
		"tslib": "2.6.2",
		"typescript": "5.4.5"
	},
	"type": "module"
}