{
	"name": "sveltekit-template",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"lint": "concurrently -g \"npm:lint:svelte\" \"npm:lint:ts\" \"npm:lint:eslint\" \"npm:lint:css\"",
		"lint:svelte": "svelte-check --tsconfig ./tsconfig.json",
		"lint:css": "stylelint \"src/**/*.{svelte,scss}\" --fix",
		"lint:ts": "tsc --noemit",
		"lint:eslint": "eslint \"src/**/*.{svelte,ts}\" --fix",
		"format": "prettier --write \"src/**/*.{svelte,ts,css,scss,html}\"",
		"kit:sync": "svelte-kit sync"
	},
	"dependencies": {
		"feather-icons": "^4.29.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.16.1",
		"@total-typescript/ts-reset": "^0.4.2",
		"@types/feather-icons": "^4.29.1",
		"@types/node": "^18.16.4",
		"@typescript-eslint/eslint-plugin": "^5.59.2",
		"@typescript-eslint/parser": "^5.59.2",
		"autoprefixer": "^10.4.14",
		"concurrently": "^8.0.1",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-svelte": "^2.27.3",
		"postcss": "^8.4.23",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"sass": "^1.62.1",
		"stylelint": "^14.16.1",
		"stylelint-config-html": "^1.1.0",
		"stylelint-config-idiomatic-order": "^9.0.0",
		"stylelint-config-prettier-scss": "^0.0.1",
		"stylelint-config-standard-scss": "^6.1.0",
		"stylelint-order": "^6.0.3",
		"stylelint-scss": "^5.0.0",
		"svelte": "^3.58.0",
		"svelte-check": "^3.3.1",
		"svelte-preprocess": "^5.0.3",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4"
	},
	"type": "module"
}